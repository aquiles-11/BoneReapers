<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[CleaningThinkTreeExtension]========== -->
	<ThinkTreeDef>
		<defName>GIVETHISAUNIQUEDEFNAME</defName>
		<insertTag>Animal_PreWander</insertTag>
		<insertPriority>1</insertPriority>
		<thinkRoot Class="ThinkNode_ConditionalPawnKind">
			<pawnKind>PAWNKINDDEF</pawnKind>
			<subNodes>
				<li Class="ThinkNode_ConditionalHasFaction">
					<subNodes>
						<!-- Trained behaviour: Clean -->
						<li Class="ThinkNode_ChancePerHour_Constant">
							<mtbHours>1.5</mtbHours>
							<subNodes>
								<li Class="ThinkNode_ConditionalRequireCapacities">
									<requiredCapacities>
										<li>Manipulation</li>
									</requiredCapacities>
									<subNodes>
										<li Class="ThinkNode_ConditionalTrainableCompleted">
											<trainable>Haul</trainable>
											<subNodes>
												<li Class="ThinkNode_Tagger">
													<tagToGive>TrainedAnimalBehavior</tagToGive>
													<subNodes>
														<li Class="AnimalCleaning.JobGiver_Clean"/>
													</subNodes>
												</li>
											</subNodes>
										</li>
									</subNodes>
								</li>
							</subNodes>
						</li>
					</subNodes>
				</li>
			</subNodes>
		</thinkRoot>
	</ThinkTreeDef>
</Defs>
