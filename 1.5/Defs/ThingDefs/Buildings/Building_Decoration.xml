<Defs>

  <!-- Osseous pillar -->

  <ThingDef ParentName="BuildingBase">
    <defName>pphhyy_OsseousPillar</defName>
    <label>osseous pillar</label>
    <description>Although BoneReapers are undead they can still appreciate art and create it. A Large pillar to adorn your halls with.</description>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <uiIconScale>0.6</uiIconScale>
    <selectable>true</selectable>
    <fillPercent>0.20</fillPercent>
