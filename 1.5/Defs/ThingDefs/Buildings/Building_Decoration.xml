<!-- Osseous Brazier -->

<ThingDef ParentName="BuildingBase">
  <defName>pphhyy_OsseousBrazier</defName>
  <label>osseous brazier</label>
  <description>A Brazier to light the halls fueled by wood.</description>
  <altitudeLayer>Building</altitudeLayer>
  <passability>PassThroughOnly</passability>
  <selectable>true</selectable>
  <fillPercent>0.20</fillPercent>
  <defaultPlacingRot>South</defaultPlacingRot>
  <pathCost>14</pathCost>
  <tickerType>Normal</tickerType>
  <drawerType>RealtimeOnly</drawerType>
  <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
  <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
  <designationCategory>Furniture</designationCategory>
  <rotatable>false</rotatable>
  <size>(1,1)</size>
  <graphicData>
    <graphicClass>Graphic_Multi</graphicClass>
    <texPath>Buildings/OsseousBrazier/OsseousBrazier</texPath>
    <shaderType>CutoutComplex</shaderType>
    <drawSize>(1.5,1.5)</drawSize>
    <drawOffset>(0,0,0.2)</drawOffset>
  </graphicData>
  <statBases>
    <MaxHitPoints>100</MaxHitPoints>
    <Flammability>0.5</Flammability>
  </statBases>
  <costStuffCount>20</costStuffCount>
  <stuffCategories>
    <li>Metallic</li>
    <li>Stony</li>
    <li>Woody</li>
  </stuffCategories>
  <uiIconScale>0.8</uiIconScale>
  <costList>
    <pphhyy_HumanBones>5</pphhyy_HumanBones>
  </costList>
  <comps>
    <li Class="CompProperties_Refuelable">
      <fuelConsumptionRate>2.0</fuelConsumptionRate>
      <fuelCapacity>30.0</fuelCapacity>
      <fuelConsumptionPerTickInRain>0.0006</fuelConsumptionPerTickInRain>
      <fuelFilter>
        <thingDefs>
          <li>WoodLog</li>
        </thingDefs>
      </fuelFilter>
      <initialFuelPercent>0</initialFuelPercent>
      <showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
    </li>
    <li Class="CompProperties_Glower">
      <glowRadius>10</glowRadius>
      <glowColor>(252,187,113,0)</glowColor>
    </li>
    <li Class="CompProperties_HeatPusher">
      <compClass>CompHeatPusherPowered</compClass>
      <heatPerSecond>3.5</heatPerSecond>
      <heatPushMaxTemperature>23</heatPushMaxTemperature>
    </li>
    <li Class="CompProperties_FireOverlay">
      <fireSize>0.6</fireSize>
      <offset>(0,0,0.4)</offset>
    </li>
    </comps>
    <researchPrerequisites>
      <li>pphhyy_BoneReaperBasicPillars</li>
    </researchPrerequisites>
</ThingDef>

