<!-- Osseous Throne -->

<ThingDef ParentName="BuildingBase">
  <defName>pphhyy_OsseousThrone</defName>
  <label>osseous throne</label>
  <description>Seat of power for the ruling BoneReaper.</description>
  <possessionCount>1</possessionCount>
  <designationCategory>Furniture</designationCategory>
  <minifiedDef>MinifiedThing</minifiedDef>
  <defaultPlacingRot>South</defaultPlacingRot>
  <thingCategories>
    <li>BuildingsFurniture</li>
  </thingCategories>
  <hasInteractionCell>true</hasInteractionCell>
  <thingClass>Building_Throne</thingClass>
  <socialPropernessMatters>true</socialPropernessMatters>
  <uiIconScale>0.7</uiIconScale>
  <altitudeLayer>Building</altitudeLayer>
  <fillPercent>0.40</fillPercent>
  <statBases>
    <MaxHitPoints>40</MaxHitPoints>
    <WorkToBuild>500</WorkToBuild>
    <Mass>5</Mass>
    <Flammability>1.0</Flammability>
    <Beauty>5</Beauty>
  </statBases>
  <stuffCategories>
    <li>Metallic</li>
    <li>Stony</li>
    <li>Woody</li>
  </stuffCategories>
  <costStuffCount>45</costStuffCount>
  <canOverlapZones>false</canOverlapZones>
  <pathCost>30</pathCost>
  <rotatable>true</rotatable>
  <building>
    <isInert>true</isInert>
    <ai_chillDestination>false</ai_chillDestination>
  </building>
  <size>(3,2)</size>
  <interactionCellOffset>(0,0,1)</interactionCellOffset>
  <graphicData>
    <texPath>Buildings/OsseousThrone/OsseousThrone</texPath>
    <graphicClass>Graphic_Multi</graphicClass>
    <shaderType>CutoutComplex</shaderType>
    <drawSize>(3.5,3.5)</drawSize>
    <drawOffset>(0,0,0.6)</drawOffset>
  </graphicData>
  <researchPrerequisites>
    <li>pphhyy_BoneReaperGrandFurniture</li>
  </researchPrerequisites>
  <ritualFocus>
    <spectateDistance>2</spectateDistance>
    <allowedSpectateSides>Up</allowedSpectateSides>
  </ritualFocus>
  <placeWorkers>
    <li>PlaceWorker_RitualFocus</li>
  </placeWorkers>
  <drawPlaceWorkersWhileSelected>True</drawPlaceWorkersWhileSelected>
  <comps>
    <li Class="CompProperties_AssignableToPawn">
      <drawAssignmentOverlay>true</drawAssignmentOverlay>
      <compClass>CompAssignableToPawn_Throne</compClass>
      <noAssignablePawnsDesc>Throne ownership requires a person with a royal title.</noAssignablePawnsDesc>
    </li>
    <li Class="CompProperties_MeditationFocus">
      <statDef>MeditationFocusStrength</statDef>
