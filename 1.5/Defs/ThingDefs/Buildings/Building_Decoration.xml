    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <Flammability>0.5</Flammability>
    </statBases>
    <costStuffCount>150</costStuffCount>
    <stuffCategories>
      <li>Metallic</li>
      <li>Stony</li>
      <li>Woody</li>
    </stuffCategories>
    <costList>
      <pphhyy_HumanBones>20</pphhyy_HumanBones>
    </costList>
    <researchPrerequisites>
      <li>pphhyy_BoneReaperGrandPillars</li>
